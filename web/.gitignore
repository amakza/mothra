# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep
!/tmp/sockets
/tmp/sockets/*
!/tmp/sockets/.keep
!/tmp/pids
/tmp/pids/*
!/tmp/pids/.keep

# Ignore uploaded files in development
/storage/*

/node_modules
/yarn-error.log

/public/assets
.byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore bundler packages.
/vendor/bundle

# No DS_Store please
.DS_Store

# Ignore schemafile
/db/schema.rb
